<!DOCTYPE html>
<html>
  <head>
    <title>WebTutsPlus WebCon</title>
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css"
      integrity="sha512-iecdLmaskl7CVkqkXNQ/ZH/XLlvWZOJyj7Yy7tcenmpD1ypASozpmT/E0iPtmFIB46ZmdtAc9eNBvH0H/ZpiBw=="
      crossorigin="anonymous"
      referrerpolicy="no-referrer"
    />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-9ndCyUaIbzAi2FUVXJi0CjmCapSmO7SnpJef0486qhLnuZ2cdeRhO02iuK6FUUVM" crossorigin="anonymous">
 
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link
      href="https://fonts.googleapis.com/css2?family=Baloo+Tamma+2:wght@400;500;600&family=Josefin+Slab&display=swap"
      rel="stylesheet"
    />
    <link rel="stylesheet" href="/css/styles.css" />
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-geWF76RCwLtnZ8qwWowPQNguL3RmwHVBC9FhGdlKrxdiJJigb/j/68SIy3Te4Bkz" crossorigin="anonymous"></script>
 

    <!-- <link rel="stylesheet" href="/css/main.css" /> -->
  </head>

  <body class="h-100">
    <div class="main">
      <div class="main_left">
        <div class="video">
          <div class="h-100" id="video_display">
            <div
              id="video_container"
              class="align-items-center"
              style="margin-top: 10%"
            >
                <video
                  id="localVideo"
                  class=""
                  autoplay
                  muted
                  playsinline
                ></video>
            </div>
          </div>
        </div>
        
        <div class="controls">
            <div class="blocks">
                <div class="block1">
                    <i class="fa-solid fa-microphone"></i>
                    <span>Mute</span>
                </div>
                <div class="block1">
                    <i class="fa-solid fa-camera" style="width: 50px;"></i>
                    <span>Stop Video</span>
                </div>
            </div>
            <div class="blocks">
                <div class="block1">
                  <i class="fa-solid fa-shield-halved"></i>
                    <span>security</span>
                </div>
                <div class="block1">
                  <i class="fa-solid fa-people-group"></i>
                    <span>participant</span>
                </div>
                <div class="block1">
                  <i class="fa-solid fa-comments"></i>
                    <span>chat</span>
                </div>
            </div>
            <div class="blocks">
                <div class="block1">
                    <span class="Meeting">Leave Meeting</span>
                </div>
            </div>
        </div>
      </div>
      <div class="main_right">
        <div class="chat">
          <h2>Chat</h6>
        </div>
        <div class="chat_window">
            <ul class="messages">

            </ul>
        </div>
        <div class="container">
            <input id="messages" type="text" placeholder="Type message here">
        </div>
      </div>
    </div>

    <!-- Import SocketIO for signalling -->
    <script src="/socket.io/socket.io.js"></script>

    <!-- Import WebRTC adapter for compatibility with all the browsers  -->
    <script src="https://webrtc.github.io/adapter/adapter-latest.js"></script>

    <!-- Import TURN config -->
    <script src="js\config.js"></script>

    <!-- Import script containing WebRTC related functions -->
    <script src="js\main.js"></script>
  </body>
</html>
